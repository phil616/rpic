{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "loguru": {
        "format": "{time:YYYY-MM-DD HH:mm:ss} | {level} | {message}",
        "colorize": true
      }
    },
    "handlers": {
      "loguru": {
        "class": "logging.StreamHandler",
        "formatter": "loguru"
      }
    },
    "root": {
      "handlers": ["loguru"],
      "level": "INFO"
    }
  }